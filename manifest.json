{
    "name": "Local Storage Blacklist",
    "version": "1.0",
    "description": "Blacklist certain sites from using your local storage",
    "permissions": [
      "activeTab",
      "declarativeContent",
      "storage",
      "webNavigation",
      "http://*/",
      "https://*/"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "options_page": "options.html",
   "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "manifest_version": 2
  }
